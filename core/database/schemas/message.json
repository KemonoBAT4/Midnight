{
    "tableName": "notes",
    "id": {
        "type": "integer",
        "primaryKey": true,
        "autoIncrement": true
    },
    "content": {
        "type": "text",
        "required": false
    },
    "sendAt": {
        "type": "datetime",
        "required": true
    },
    "chat": {
        "type": "integer",
